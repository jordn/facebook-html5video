{
  "manifest_version": 2,

  "name": "Facebook HTML5 Video",
  "description": "Replace the flash video player with HTML5 videos on Facebook.",
  "version": "1.0",
  "icons": {  },

  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ],

  "permissions": [
    "https://facebook.com/"
  ],

  "web_accessible_resources": [
  ]
}
